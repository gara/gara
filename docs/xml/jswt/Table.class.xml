						<params>
							<param type="gara.jswt.TreeItem" name="item">
								<description>the item to deselect</description>
							</param>
						</params>
						
						<throws></throws>
						
						
						<return type="void"></return>
						
						
						<authors>
							<author>Thomas Gossmann</author>
						</authors>
						
						<sees></sees>
						
						
						
					</method>
				
